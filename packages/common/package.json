{
  "name": "@hydra-ipxe/common",
  "version": "0.0.0",
  "description": "Common code used between the hydra-ipxe server and web",
  "license": "MIT",
  "author": "Szymon Kozio≈Ç <contact@critteros.dev>",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": [
      "./dist/*.js",
      "./dist/*/index.js"
    ]
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "prettier": "@hydra-ipxe/prettier-config",
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@hydra-ipxe/eslint-config-hydra": "workspace:*",
    "@hydra-ipxe/prettier-config": "workspace:*",
    "@hydra-ipxe/tsconfig": "workspace:*",
    "tsup": "^7.2.0"
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ]
      }
    }
  }
}
