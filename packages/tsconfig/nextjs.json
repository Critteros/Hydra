{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": ["./base.json", "@tsconfig/next/tsconfig.json"],
  "compilerOptions": {
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "jsx": "preserve",
    "module": "esnext",
    "noEmit": true,
    "resolveJsonModule": true,
    "strict": false,
    "target": "es5",
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  }
}
